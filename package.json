{
    "name": "relay-syntax",
    "displayName": "Relay syntax",
    "description": "Syntax highlight support for the relay scripting language",
    "icon": "res/round.png",
    "repository": {"url": "https://github.com/fabricioh/relay-syntax-vscode"},
    "publisher": "fabricioh",
    "version": "3.0.0",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "relay",
            "aliases": ["Relay", "relay"],
            "extensions": [".ry"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "relay",
            "scopeName": "source.relay",
            "path": "./syntaxes/relay.tmLanguage.json"
        }]
    }
}